{
  "name": "proxim-ai-api",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "test": "jest --testTimeout=15000 --forceExit --runInBand --silent",
    "test:watch": "jest --watch --testTimeout=15000 --forceExit --runInBand --silent",
    "test:single": "./scripts/exec_single_test.sh",
    "start": "nodemon ./src",
    "notifications:client": "http-server public",
    "docker:login": "aws ecr get-login-password --region eu-central-1 --profile proxim | docker login --username AWS --password-stdin 125400652915.dkr.ecr.eu-central-1.amazonaws.com",
    "docker:build": "docker build --no-cache -t 125400652915.dkr.ecr.eu-central-1.amazonaws.com/proxim-api-v3:development .",
    "docker:run": "docker run -d --name proxim-api -p 3010:3000 125400652915.dkr.ecr.eu-central-1.amazonaws.com/proxim-api-v3:development",
    "docker:push": "docker push 125400652915.dkr.ecr.eu-central-1.amazonaws.com/proxim-api-v3:development",
    "openapi:publish": "swagger-cli validate openapi/api.v1.yaml && swagger-cli bundle openapi/api.v1.yaml -o src/openapi.json --dereference && swagger-cli bundle openapi/api.v1.yaml -o src/openapi.yaml --dereference -t yaml"
  },
  "dependencies": {
    "@kafkajs/confluent-schema-registry": "^1.0.6",
    "aws-sdk": "^2.890.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compressing": "^1.5.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^24.3.2",
    "express": "~4.16.1",
    "express-openapi-validator": "^4.10.1",
    "express-fileupload": "^1.2.1",
    "firebase-admin": "^9.5.0",
    "fs-extra": "^10.0.0",
    "helmet": "^4.3.1",
    "http-errors": "~1.6.3",
    "i18n": "^0.13.2",
    "ipfs-http-client": "^52.0.0",
    "js2xmlparser": "^4.0.1",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.15.0",
    "keycloak-admin": "^1.14.7",
    "keycloak-connect": "^12.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "mongoose-paginate-v2": "^1.3.17",
    "nanoid": "^3.1.22",
    "nodemailer": "^6.4.18",
    "openid-client": "^4.4.0",
    "pug": "2.0.0-beta11",
    "qs": "^6.9.6",
    "repeat-promise-until-resolved": "^1.0.9",
    "resize-img": "^2.0.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.61.0",
    "validator": "^13.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "jest-openapi": "^0.11.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.0.1"
  },
